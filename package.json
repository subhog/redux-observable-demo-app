{
  "name": "redux-observable-demo-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.3",
    "@material-ui/icons": "^4.9.1",
    "@reduxjs/toolkit": "^1.2.5",
    "immer": "^5.3.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.3.3",
    "react-redux": "^7.2.0",
    "react-scripts": "3.3.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.5.4",
    "styled-components": "^5.0.1",
    "typescript": "^3.8.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "jest --verbose",
    "test-watch": "jest --watch",
    "test-watch-all": "jest --watchAll --coverage",
    "eject": "react-scripts eject",
    "server": "json-server --watch db.json --port 5000 --delay 5000",
    "format": "prettier --config ./.prettierrc.js --check 'src/**/*' 'assets/**/*.json' --write",
    "lint": "eslint ./src/**/*.{ts,tsx} --fix",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "plop": "plop --plopfile .plop/plopfile.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addons": "^5.3.13",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.13",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.28",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/redux-logger": "^3.0.7",
    "@types/styled-components": "^4.4.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript-prettier": "^1.4.2",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-jest": "^23.7.0",
    "husky": "^4.2.3",
    "jest": "25.1.0",
    "jest-cli": "^25.1.0",
    "jest-watch-typeahead": "^0.4.2",
    "json-server": "^0.15.1",
    "lint-staged": "^10.0.7",
    "plop": "^2.5.4",
    "prettier": "^1.19.1",
    "react-app-rewired": "^2.1.5",
    "ts-jest": "^25.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "prettier --write --check"
    ],
    "public/**/*.json": [
      "prettier --write --check"
    ]
  }
}
